{
  "name": "safetag-website",
  "private": true,
  "description": "This project creates a web-based version of the full SAFETAG (Security Auditing Framework and Evaluation Template for Advocacy Groups) guide produced by Internews.",
  "version": "0.1.0",
  "author": "Development Seed",
  "dependencies": {
    "@reach/listbox": "^0.17.0",
    "@reach/visually-hidden": "^0.17.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "blob-stream": "^0.1.3",
    "browser-monads": "^1.0.0",
    "dotenv": "^16.0.1",
    "file-saver": "^2.0.5",
    "gatsby": "^4.20.0",
    "gatsby-cli": "^4.20.0",
    "gatsby-plugin-feed": "^4.20.0",
    "gatsby-plugin-flexsearch": "^1.0.3",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-matomo": "^0.13.0",
    "gatsby-plugin-netlify-cms": "^6.20.0",
    "gatsby-plugin-offline": "^5.20.0",
    "gatsby-plugin-polished": "^1.0.4",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-react-i18next": "^2.0.4",
    "gatsby-plugin-styled-components": "^5.20.0",
    "gatsby-remark-images": "^6.20.0",
    "gatsby-source-filesystem": "^4.20.0",
    "gatsby-transformer-remark": "^5.20.0",
    "hast-util-to-html": "^8.0.3",
    "i18next": "^21.9.0",
    "i18next-parser": "^6.5.0",
    "lodash.get": "^4.4.2",
    "lodash.keyby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.pickby": "^4.6.0",
    "lodash.values": "^4.3.0",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-core": "^2.55.2",
    "pdfkit": "^0.13.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.18.4",
    "react-remark": "^2.1.0",
    "rehype-react": "^7.1.1",
    "remark-html": "^15.0.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.5",
    "transifex": "^1.6.6",
    "use-query-params": "^2.0.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "brfs": "^2.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-inclusive-language": "^2.2.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-remark-sectionize": "^1.0.0",
    "gh-pages": "^4.0.0",
    "marked": "^4.0.18",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "build-deploy": "gatsby build --prefix-paths",
    "deploy-gh-pages": "gatsby build --prefix-paths && gh-pages -d public -b guide",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "extract": "i18next 'src/pages/*.js' 'src/components/*.js' 'src/components/layouts/*.js' 'src/helpers/*.js'",
    "transifex-push": "tx init --no-interactive; tx config mapping -r safetag-overview.site --source-lang en --type json --source-file locales/en/site.json --expression 'locales/<lang>/site.json' --execute && tx config mapping-bulk -p safetag-overview --source-language en --type GITHUBMARKDOWN -f '.md' --source-file-dir content/ --expression 'locales/<lang>/{filepath}/{filename}{extension}' --ignore-dir posts --execute && tx push -s --parallel --skip --use-git-timestamps --no-interactive",
    "transifex-pull": "node -r dotenv/config src/helpers/transifex.js && python3 src/helpers/postprocess.py",
    "postproc": "python3 src/helpers/postprocess.py"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/safetag-web"
  },
  "overrides": {
    "ansi-html": ">=0.0.8",
    "trim": ">=0.0.3",
    "minimist": ">=1.2.6",
    "glob-parent": ">=5.1.2",
    "multer": ">=1.4.5-lts.1",
    "diff": ">=3.5.0",
    "jpeg-js": ">=0.4.4",
    "node-fetch": "^2.6.6",
    "graphql": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "got": "^11.8.5",
    "@parcel/core": "^2.6.2",
    "parse-path": "^5.0.0",
    "cross-fetch": "^3.1.5",
    "ws": "^7.4.6",
    "sanitize-html": "^2.3.1",
    "svgo": "^2.0.0"
  }
}
