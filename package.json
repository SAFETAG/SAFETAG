  "scripts": {
    "build": "npm run transifex && gatsby build",
    "develop": "npm run transifex && gatsby develop",
    "build-deploy": "npm run transifex && gatsby build --prefix-paths",
    "deploy-gh-pages": "gatsby build --prefix-paths && gh-pages -d public -b guide",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "extract": "i18next 'src/pages/*.js'",
    "transifex": "node -r dotenv/config src/helpers/transifex.js"
